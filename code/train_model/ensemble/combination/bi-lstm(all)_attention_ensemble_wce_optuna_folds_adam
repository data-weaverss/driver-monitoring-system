n","        \"persistent_workers\": cfg.use_cuda,\n","        \"prefetch_factor\": 2 if cfg.use_cuda else None,\n","        \"drop_last\": True,\n","    }\n","\n","    test_params = {\n","        \"batch_size\": batch_size,\n","        \"shuffle\": False,\n","        \"num_workers\": 0,\n","        \"pin_memory\": cfg.use_cuda\n","    }\n","\n","    for fold in range(5):\n","        print(f\"\\n===== Fold {fold} =====\")\n","\n","        # === 1. Fold split ===\n","        train_fold_df = train_df[train_df['fold'] != fold]\n","        val_fold_df   = train_df[train_df['fold'] == fold]\n","\n","        # === 2. DataLoader ì¤€ë¹„ ===\n","        train_loader = get_augmented_train_loader(train_fold_df, params=train_params)\n","        val_dataset, _ = generate_sequence_dataset(val_fold_df)\n","        val_loader = DataLoader(val_dataset, **test_params)\n","\n","        # === 3. ëª¨ë¸ ë° ì˜µí‹°ë§ˆì´ì € ì •ì˜ ===\n","        model = DecoderRNN(cfg.lstm_hidden_size, cfg.fc_hidden_size, dropout_rate).to(cfg.device)\n","        optimizer = get_optimizer(model, optimizer_name, lr)\n","\n","        # === 4. Class Weight ê³„ì‚° ===\n","        class_counts = train_fold_df['label'].value_counts()\n","        weights = [1.0 / class_counts.get(label, 1) for label in cfg.labels]\n","        class_weights = torch.tensor(weights, dtype=torch.float32).to(cfg.device)\n","        criterion = get_criterion(class_weights=class_weights)\n","\n","        # === 5. í•™ìŠµ ë° í‰ê°€ ===\n","        _, _, val_losses, _ = train_and_validate(\n","            model, train_loader, val_loader, optimizer, criterion, fold,\n","            earlystop_patience=30, epochs=30,\n","            scheduler=None\n","        )\n","\n","        all_val_losses.append(min(val_losses))  # foldë³„ ìµœì†Œ val_loss ê¸°ë¡\n","\n","    # í‰ê· ê³¼ í‘œì¤€í¸ì°¨ ê³„ì‚°\n","    mean_loss = np.mean(all_val_losses)\n","    std_loss = np.std(all_val_losses)\n","\n","    # í‰ê·  + í‘œì¤€í¸ì°¨ë¥¼ ë°˜í™˜í•´ í‰ê·  ì„±ëŠ¥ê³¼ ì•ˆì •ì„±ì„ ë™ì‹œì— ê³ ë ¤\n","    objective_value = mean_loss + std_loss\n","    print(f\"Fold mean val_loss: {mean_loss:.4f}, std: {std_loss:.4f}, objective: {objective_value:.4f}\")\n","\n","    return objective_value"]},{"cell_type":"code","execution_count":22,"metadata":{"colab":{"base_uri":"https://localhost:8080/"},"executionInfo":{"elapsed":11226600,"status":"ok","timestamp":1751470683128,"user":{"displayName":"Sol Lee","userId":"06456628920237989185"},"user_tz":-540},"id":"zrfI-URrusgn","outputId":"2a923b86-0634-4adc-9218-c6c77dc92456"},"outputs":[{"output_type":"stream","name":"stderr","text":["[I 2025-07-02 12:30:56,552] A new study created in memory with name: bi_lstm_attention_tuning_adamw\n"]},{"output_type":"stream","name":"stdout","text":["\n","===== Fold 0 =====\n","\n","[Step 1] Original class counts: Counter({np.int64(0): 1319, np.int64(1): 685, np.int64(2): 491, np.int64(3): 159})\n","[Yaw Flip] Class 2: 491 â†’ 736 (ì¶”ê°€ 245)\n","[Yaw Flip] Class 3: 159 â†’ 238 (ì¶”ê°€ 79)\n","[Step 2] After Yaw Flip class counts: Counter({np.int64(0): 1319, np.int64(2): 736, np.int64(1): 685, np.int64(3): 238})\n","[Step 3] Sampling strategy for ROS: {np.int64(2): 1104, np.int64(3): 357}\n","Original class counts: Counter({np.int64(0): 1319, np.int64(2): 736, np.int64(1): 685, np.int64(3): 238})\n","Sampling strategy for ROS: {np.int64(2): 1104, np.int64(3): 357}\n","[Step 4] Final class distribution after ROS: Counter({np.int64(0): 1319, np.int64(2): 1104, np.int64(1): 685, np.int64(3): 357})\n","Final class distribution: Counter({np.int64(0): 1319, np.int64(2): 1104, np.int64(1): 685, np.int64(3): 357})\n","âœ… Using Weighted CrossEntropyLoss\n"]},{"output_type":"stream","name":"stderr","text":["Epoch 1 [Train]: 100%|â–ˆâ–ˆâ–ˆâ–ˆâ–ˆâ–ˆâ–ˆâ–ˆâ–ˆâ–ˆ| 108/108 [00:03<00:00, 32.88it/s]\n"]},{"output_type":"stream","name":"stdout","text":["âœ… Train Epoch 1 Finished - Avg Loss: 0.2612, Avg Acc: 83.56%\n","âœ… Validation Finished - Avg Loss: 0.1992, Avg Acc: 94.62%\n","ğŸ