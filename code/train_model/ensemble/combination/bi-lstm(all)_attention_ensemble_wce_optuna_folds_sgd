_window_step = { #ìŠ¬ë¼ì´ë”© ìœˆë„ìš° ì„¤ì •\n","    'normal': 24,\n","    'phonecall': 24,\n","    'text': 12,\n","    'yawn': 12\n","    }\n","    features: list = field(default_factory=lambda: [\"mar\", \"ear\", \"yaw\", \"pitch\", \"phone_detected\"])\n","    labels: list = field(default_factory=lambda: [\"normal\", \"phonecall\", \"text\", \"yawn\"])\n","\n","    # LossFunction ì„¤ì •\n","    loss_mode: str = \"wce\" # \"ce\", \"wce\", \"focal\"\n","\n","    apply_yaw_inversion: bool = False\n","    apply_noise_jittering: bool = False\n","\n","    # í•™ìŠµ ì œì–´ - ì¡°ê¸° ì¢…ë£Œ / ëŸ¬ë‹ë ˆì´íŠ¸ ìŠ¤ì¼€ì¤„ëŸ¬\n","    use_early_stopping: bool = True  # ì¡°ê¸° ì¢…ë£Œ ì‚¬ìš© ì—¬ë¶€\n","    use_lr_scheduler: bool = False    # í•™ìŠµë¥  ìŠ¤ì¼€ì¤„ëŸ¬ ì‚¬ìš© ì—¬ë¶€\n","    early_stopping_patience: int = 20 # early stopping ê¸°ì¤€ epoch\n","    reduce_lr_factor: float = 0.5 # learning rate ê°ì†Œ ë¹„ìœ¨\n","    reduce_lr_patience: int = 7 # learning rate ê°ì†Œ ê¸°ì¤€ epoch\n","\n","    # ëœë¤ì‹œë“œ ì„¤ì •\n","    random_seed: int = 24\n","\n","    def __post_init__(self):\n","      self.results_save_path = f\"{base_dir}/runs/{self.case_name}\"\n","      self.model_save_path = f\"{self.results_save_path}/weights\"\n","\n","      os.makedirs(self.results_save_path, exist_ok=True)\n","      os.makedirs(self.model_save_path, exist_ok=True)\n","\n","      if self.sequence_length == 12:\n","        df_path = f'{base_dir}/data/splits/12frame_windows'\n","        self.group_col = 'groups_12'\n","      elif self.sequence_length == 24:\n","        df_path = f'{base_dir}/data/splits/24frame_windows'\n","        self.group_col = 'groups_24'\n","\n","      self.train = pd.read_csv(os.path.join(df_path, \"final_train.csv\"))\n","      self.validation = pd.read_csv(os.path.join(df_path, \"final_val.csv\"))\n","      self.test = pd.read_csv(os.path.join(df_path, \"final_test.csv\"))\n","\n","      # label_mapping\n","      self.label_encoding = {}\n","      self.label_decoding = {}\n","      for i, label in enumerate(self.labels): # [\"normal\", \"phonecall\", \"text\", \"yawn\"] ìˆœì„œë¡œ ë¼ë²¨ ì¸ì½”ë”©\n","          self.label_encoding[label] = i\n","          self.label_decoding[i] = label\n","\n","      # ë””ë°”ì´ìŠ¤ ì„¤ì •\n","      self.use_cuda = torch.cuda.is_available()\n","      self.device = torch.device(\"cuda\" if self.use_cuda else \"cpu\")\n","\n","cfg = Config()\n","# print(cfg.case_name) # ì‚¬ìš© ì˜ˆì‹œ"],"metadata":{"id":"5hKlc4kUp4l9","executionInfo":{"status":"ok","timestamp":1751349603867,"user_tz":-540,"elapsed":11523,"user":{"displayName":"Sol Lee","userId":"04508513172834796855"}}},"execution_count":4,"outputs":[]},{"cell_type":"code","source":["def set_random_seed(seed):\n","    torch.manual_seed(seed)  # PyTorch CPU ì—°ì‚°ì— ëŒ€í•œ ì‹œë“œ ì„¤ì •\n","    torch.cuda.manual_seed(seed)  # CUDA ì—°ì‚°ì— ëŒ€í•œ ì‹œë“œ ì„¤ì • (GPU ì‚¬ìš© ì‹œ)\n","    torch.cuda.manual_seed_all(seed)  # ì—¬ëŸ¬ GPUë¥¼ ì‚¬ìš©í•˜ëŠ” ê²½ìš° ëª¨ë“  GPUì— ë™ì¼í•œ ì‹œë“œ ì„¤ì •\n","    np.random.seed(seed)  # NumPy ì—°ì‚°ì— ëŒ€í•œ ì‹œë“œ ì„¤ì •\n","    random.seed(seed)  # Python ê¸°ë³¸ random ëª¨ë“ˆì˜ ì‹œë“œ ì„¤ì •\n","    torch.backends.cudnn.deterministic = True  # CuDNN ì—°ì‚°ì˜ ê²°ì •ë¡ ì  ë™ì‘ ë³´ì¥\n","    torch.backends.cudnn.benchmark = False  # ì„±ëŠ¥ ìµœì í™” ë¹„í™œì„±í™” (ì¬í˜„ì„± ë³´ì¥)\n","\n","set_random_seed(cfg.random_seed)"],"metadata":{"id":"nnUAKflRWPoq","executionInfo":{"status":"ok","timestamp":1751349603868,"user_tz":-540,"elapsed":7,"user":{"displayName":"Sol Lee","userId":"04508513172834796855"}}},"execution_count":5,"outputs":[]},{"cell_type":"markdown","metadata":{"id":"pRHMOaLpW7Y2"},"source":["### ë°ì´í„° ë¡œë“œ"]},{"cell_type":"markdown","source":["### train, val, test ë°ì´í„°ì…‹ ë¡œë“œ"],"metadata":{"id":"R1vpZJH9TRky"}},{"cell_type":"code","source":["train_df = cfg.train.copy()\n","val_df = cfg.validation.copy()\n","test_df = cfg.test.copy()"],"metadata":{"id":"LaskMuLUQ89h","executionInfo":{"status":"ok","timestamp":1751349603869,"user_tz":-540,"elapsed":0,"user":{"displayName":"Sol Lee","userId":"04508513172834796855"}}},"execution_count":6,"outputs":[]},{"cell_type":"code","source":["# train normal undersampling\n","train_df = train_df[((train_df['label'] == 'normal') &\n","                    (train_df[cfg.group_col].isin([0, 1]))) |\n","                    (train_df['label'] != 'normal')]"],"metadata":{"id":"eHUoDw4U9kQR","executionInfo":{"status":"ok","timestamp":1751349603871,"user_tz":-540,"elapsed":1,"user":{"displayName":"Sol Lee","userId":"04508513172834796855"}}},"execution_count":7,"outputs":[]},{"cell_type":"markdown","metadata":{"id":"OdLSCKeSrZAE"},"source":["### generate_sequence_dataset"]},{"cell_type":"code","source":["def generate_sequence_dataset(\n","    df: pd.DataFrame,\n","    use_test_mode: bool = True #í•™ìŠµìš©ê³¼ í…ŒìŠ¤íŠ¸ìš© ìŠ¬ë¼ì´ë”© ìœˆë„ìš° ë‹¤ë¥´ê²Œ ì„¤ì •. ê¸°ë³¸ê°’ì€ ìŠ¬ë¼ì´ë”© ìœˆë„ìš° ì ìš©x\n",") -> Tuple[TensorDataset, List[Tuple]]:\n","    sequences, labels, metadata = [], [], []\n","\n","    for (label, clip_id), clip_df in df.groupby(['label', 'clip_id']):\n","        clip_df = clip_df.sort_values('frame_no')\n","        frame_values = clip_df[cfg.features].values\n","\n","        if len(frame_values) < cfg.sequence_length:\n","            continue\n","\n","        # ìŠ¬ë¼ì´ë”© ìœˆë„ìš° ì„¤ì •\n","        step = cfg.sequence_length if use_test_mode else cfg.sliding_window_step[label]\n","\n","        for i in range(0, len(frame_values) - cfg.sequence_length + 1, step):\n","            seq = frame_values[i:i + cfg.sequence_length]\n","            if seq.shape[0] == cfg.sequence_length:\n","                sequences.append(seq)\n","                labels.append(cfg.label_encoding[label])\n","                metadata.append((label, clip_id, clip_df.iloc[i]['frame_no']))\n","\n","    return TensorDataset(torch.tensor(sequences, dtype=torch.float32), torch.tensor(labels, dtype=torch.long)), metadata\n"],"metadata":{"id":"SbY5UTS4T39f","executionInfo":{"status":"ok","timestamp":1751349603872,"user_tz":-540,"elapsed":1,"user":{"displayName":"Sol Lee","userId":"04508513172834796855"}}},"execution_count":8,"outputs":[]},{"cell_type":"markdown","metadata":{"id":"iGaRQUVjFDPA"},"source":["### DataLoader params"]},{"cell_type":"code","source":["'''\n","# DataLoader ìƒì„± (ë°°ì¹˜ ë‹¨ìœ„ë¡œ ë°ì´í„°ë¥¼ ë‚˜ëˆ„ê¸° ìœ„í•´ ì‚¬ìš©)\n","train_dataset, train_metadata = generate_sequence_dataset(train_df)\n","\n","# ê¸°ì¡´ ì‹œí€€ìŠ¤ ë° ë¼ë²¨ ë¶ˆëŸ¬ì˜¤ê¸°\n","X_seq = train_dataset.tensors[0].numpy()\n","y_seq = train_dataset.tensors[1].numpy()\n","\n","# ì¦ê°• ëŒ€ìƒ í´ë˜ìŠ¤\n","target_classes = [2, 3]  # 2: text, 3: yawn\n","yaw_index = cfg.features.index('yaw')\n","flip_multiplier = 1.5\n","\n","aug_seqs = []\n","aug_labels = []\n","\n","for cls in target_classes:\n","    class_seqs = [x for x, y in zip(X_seq, y_seq) if y == cls]\n","    current_count = len(class_seqs)\n","    target_count = int(current_count * flip_multiplier)\n","    need = target_count - current_count\n","\n","    print(f\"[Yaw Flip] Class {cls} ì¦ê°•: {current_count} â†’ {target_count} (ì¶”ê°€ {need})\")\n","\n","    sampled = np.random.choice(len(class_seqs), size=need, replace=True)\n","    for i in sampled:\n","        flipped = flip_yaw_sequence(class_seqs[i], yaw_index)\n","        aug_seqs.append(flipped)\n","        aug_labels.append(cls)\n","\n","# numpy í˜•íƒœë¡œ ë³€í™˜\n","X_aug = np.stack(aug_seqs)\n","y_aug = np.array(aug_labels)\n","\n","# ì›ë³¸ + ì¦ê°• í•©ì¹˜ê¸°\n","X_total = np.concatenate([X_seq, X_aug], axis=0)\n","y_total = np.concatenate([y_seq, y_aug], axis=0)\n","\n","# PyTorch Dataset & DataLoader ìƒì„±\n","train_dataset_yaw_flip = TensorDataset(torch.tensor(X_total, dtype=torch.float32),\n","                                       torch.tensor(y_total, dtype=torch.long))\n","train_loader = DataLoader(train_dataset_yaw_flip, **train_params)\n","'''"],"metadata":{"colab":{"base_uri":"https://localhost:8080/","height":179},"id":"HnQq6pVJUN1J","executionInfo":{"status":"ok","timestamp":1751349603892,"user_tz":-540,"elapsed":20,"user":{"displayName":"Sol Lee","userId":"04508513172834796855"}},"outputId":"4348aace-d16c-45c4-b1da-26abdb0e4562"},"execution_count":9,"outputs":[{"output_type":"execute_result","data":{"text/plain":["'\\n# DataLoader ìƒì„± (ë°°ì¹˜ ë‹¨ìœ„ë¡œ ë°ì´í„°ë¥¼ ë‚˜ëˆ„ê¸° ìœ„í•´ ì‚¬ìš©)\\ntrain_dataset, train_metadata = generate_sequence_dataset(train_df)\\n\\n# ê¸°ì¡´ ì‹œí€€ìŠ¤ ë° ë¼ë²¨ ë¶ˆëŸ¬ì˜¤ê¸°\\nX_seq = train_dataset.tensors[0].numpy()\\ny_seq = train_dataset.tensors[1].numpy()\\n\\n# ì¦ê°• ëŒ€ìƒ í´ë˜ìŠ¤\\ntarget_classes = [2, 3]  # 2: text, 3: yawn\\nyaw_index = cfg.features.index(\\'yaw\\')\\nflip_multiplier = 1.5\\n\\naug_seqs = []\\naug_labels = []\\n\\nfor cls in target_classes:\\n    class_seqs = [x for x, y in zip(X_seq, y_seq) if y == cls]\\n    current_count = len(class_seqs)\\n    target_count = int(current_count * flip_multiplier)\\n    need = target_count - current_count\\n\\n    print(f\"[Yaw Flip] Class {cls} ì¦ê°•: {current_count} â†’ {target_count} (ì¶”ê°€ {need})\")\\n\\n    sampled = np.random.choice(len(class_seqs), size=need, replace=True)\\n    for i in sampled:\\n        flipped = flip_yaw_sequence(class_seqs[i], yaw_index)\\n        aug_seqs.append(flipped)\\n        aug_labels.append(cls)\\n\\n# numpy í˜•íƒœë¡œ ë³€í™˜\\nX_aug = np.stack(aug_seqs)\\ny_aug = np.array(aug_labels)\\n\\n# ì›ë³¸ + ì¦ê°• í•©ì¹˜ê¸°\\nX_total = np.concatenate([X_seq, X_aug], axis=0)\\ny_total = np.concatenate([y_seq, y_aug], axis=0)\\n\\n# PyTorch Dataset & DataLoader ìƒì„±\\ntrain_dataset_yaw_flip = TensorDataset(torch.tensor(X_total, dtype=torch.float32),\\n                                       torch.tensor(y_total, dtype=torch.long))\\ntrain_loader = DataLoader(train_dataset_yaw_flip, **train_params)\\n'"],"application/vnd.google.colaboratory.intrinsic+json":{"type":"string"}},"metadata":{},"execution_count":9}]},{"cell_type":"markdown","source":["### ë°ì´í„° ì¦ê°• í•œë²ˆì— ì ìš©. yaw flip + ros"],"metadata":{"id":"D_tFqXTrzEb_"}},{"cell_type":"code","source":["def flip_yaw_sequence(seq: np.ndarray, yaw_index: int) -> np.ndarray:\n","    \"\"\"ì‹œí€€ìŠ¤ì˜ yaw ê°’ì„ ì¢Œìš° ë°˜ì „\"\"\"\n","    seq_flipped = seq.copy()\n","    seq_flipped[:, yaw_index] = -seq[:, yaw_index]\n","    return seq_flipped"],"metadata":{"id":"lNbxl2_fO-3B","executionInfo":{"status":"ok","timestamp":1751349603894,"user_tz":-540,"elapsed":1,"user":{"displayName":"Sol Lee","userId":"04508513172834796855"}}},"execution_count":10,"outputs":[]},{"cell_type":"code","source":["from imblearn.over_sampling import RandomOverSampler\n","from collections import Counter\n","\n","def get_augmented_train_loader(\n","    df: pd.DataFrame,\n","    params: dict,\n","    cfg = cfg,\n","    flip_multiplier: float = 1.5,\n","    flip_classes: list = [2, 3],\n","    ros_target_multipliers: dict = {2: 1.5, 3: 1.5}\n",") -> DataLoader:\n","    \"\"\"\n","    1. yaw ì¢Œìš° ë°˜ì „ì„ í†µí•œ ë„ë©”ì¸ ì¦ê°• (flip_classes ëŒ€ìƒ)\n","    2. RandomOverSamplerë¥¼ í†µí•œ ìˆ˜ì¹˜ì  ì¦ê°• (ros_target_multipliers ëŒ€ìƒ)\n","    â†’ TensorDatasetì„ ë°˜í™˜\n","    \"\"\"\n","    # === 1. ê¸°ë³¸ ì‹œí€€ìŠ¤ ë°ì´í„° ìƒì„± ===\n","    base_dataset, _ = generate_sequence_dataset(df, use_test_mode = False)\n","    X_seq = base_dataset.tensors[0].numpy()\n","    y_seq = base_dataset.tensors[1].numpy()\n","\n","    yaw_index = cfg.features.index('yaw')\n","    aug_seqs, aug_labels = [], []\n","\n","    # Print original counts\n","    orig_counts = Counter(y_seq)\n","    print(f\"\\n[Step 1] Original class counts: {orig_counts}\")\n","\n","    # === 2. yaw flip ì¦ê°• ===\n","    for cls in flip_classes:\n","        class_seqs = [x for x, y in zip(X_seq, y_seq) if y == cls]\n","        current_count = len(class_seqs)\n","        target_count = int(current_count * flip_multiplier)\n","        need = target_count - current_count\n","\n","        print(f\"[Yaw Flip] Class {cls}: {current_count} â†’ {target_count} (ì¶”ê°€ {need})\")\n","\n","        sampled = np.random.choice(len(class_seqs), size=need, replace=True)\n","        # np.random.choice(a, size, replace) 0~aê¹Œì§€ ì •ìˆ˜ ì¤‘ sizeê°œë¥¼ ë½‘ëŠ”ë‹¤. replace ì¤‘ë³µ í—ˆìš© ì—¬ë¶€\n","        for i in sampled:\n","            flipped = flip_yaw_sequence(class_seqs[i], yaw_index) # flip ì ìš©í•  í´ë˜ìŠ¤ì˜ ìƒ˜í”Œê³¼ ê±°ê¸°ì„œ yawì˜ ì¸ë±ìŠ¤\n","            aug_seqs.append(flipped)\n","            aug_labels.append(cls)\n","\n","    # === 3. yaw flip ë°ì´í„° ê²°í•© ===\n","    if aug_seqs:\n","        X_aug = np.stack(aug_seqs)\n","        y_aug = np.array(aug_labels)\n","        X_total = np.concatenate([X_seq, X_aug], axis=0)\n","        y_total = np.concatenate([y_seq, y_aug], axis=0)\n","    else:\n","        X_total = X_seq\n","        y_total = y_seq\n","\n","    after_flip_counts = Counter(y_total)\n","    print(f\"[Step 2] After Yaw Flip class counts: {after_flip_counts}\")\n","\n","    # === 4. RandomOverSampler ì ìš© ===\n","    X_flat = X_total.reshape(len(X_total), -1)  # [N, seq_len * feature_dim]\n","    class_counts = Counter(y_total)\n","\n","    sampling_strategy = {\n","        cls: int(class_counts[cls] * ros_target_multipliers.get(cls, 1.0))\n","        # ë”•ì…”ë„ˆë¦¬.get(a, b) : ë”•ì…”ë„ˆë¦¬ì— aê°€ ì¡´ì¬í•˜ë©´ aì˜ ê°’ì„, aê°€ ì—†ë‹¤ë©´ bë¥¼ ì¶œë ¥í•œë‹¤. ì„¤ì •í•œ ê°’ë§Œ ê°€ì ¸ì˜¤ê³  ë‚˜ë¨¸ì§€ëŠ” ê¸°ë³¸ê°’ìœ¼ë¡œ\n","        for cls in class_counts\n","        if cls in ros_target_multipliers\n","    }\n","    print(f\"[Step 3] Sampling strategy for ROS: {sampling_strategy}\")\n","    print(\"Original class counts:\", class_counts)\n","    print(\"Sampling strategy for ROS:\", sampling_strategy)\n","\n","    ros = RandomOverSampler(sampling_strategy=sampling_strategy, random_state=cfg.random_seed)\n","    X_resampled, y_resampled = ros.fit_resample(X_flat, y_total)\n","\n","    final_counts = Counter(y_resampled)\n","    print(f\"[Step 4] Final class distribution after ROS: {final_counts}\")\n","\n","    # === 5. TensorDataset ë° DataLoaderë¡œ ë³€í™˜ ===\n","    X_final = torch.tensor(X_resampled.reshape(-1, cfg.sequence_length, len(cfg.features)), dtype=torch.float32)\n","    y_final = torch.tensor(y_resampled, dtype=torch.long)\n","    final_dataset = TensorDataset(X_final, y_final)\n","\n","    print(\"Final class distribution:\", Counter(y_resampled))\n","\n","    return DataLoader(final_dataset, **params)\n"],"metadata":{"id":"DPxAf0K8O8wS","executionInfo":{"status":"ok","timestamp":1751349605083,"user_tz":-540,"elapsed":1189,"user":{"displayName":"Sol Lee","userId":"04508513172834796855"}}},"execution_count":11,"outputs":[]},{"cell_type":"markdown","metadata":{"id":"fROYd8X1zYa7"},"source":["## 1-3. LSTM í•™ìŠµ ì„¤ì •"]},{"cell_type":"markdown","source":["### ëª¨ë¸ ì •ì˜"],"metadata":{"id":"KnLIHJVEBYwV"}},{"cell_type":"code","source":["# Attention ëª¨ë“ˆ ì •ì˜\n","class Attention(nn.Module):\n","    def __init__(self, hidden_dim):\n","        super(Attention, self).__init__()\n","        self.attn = nn.Linear(hidden_dim, hidden_dim)\n","        self.context = nn.Linear(hidden_dim, 1, bias=False)\n","\n","    def forward(self, lstm_output):\n","        # lstm_output: [B, T, H]\n","        score = torch.tanh(self.attn(lstm_output))         # [B, T, H]\n","        attn_weights = torch.softmax(self.context(score), dim=1)  # [B, T, 1]\n","        attended_output = torch.sum(attn_weights * lstm_output, dim=1)  # [B, H]\n","        return attended_output, attn_weights  # context vector, weights"],"metadata":{"id":"LGtL2qsUnhI0","executionInfo":{"status":"ok","timestamp":1751349605085,"user_tz":-540,"elapsed":1,"user":{"displayName":"Sol Lee","userId":"04508513172834796855"}}},"execution_count":12,"outputs":[]},{"cell_type":"code","execution_count":13,"metadata":{"id":"AOK3mN2xHwtt","executionInfo":{"status":"ok","timestamp":1751349605087,"user_tz":-540,"elapsed":1,"user":{"displayName":"Sol Lee","userId":"04508513172834796855"}}},"outputs":[],"source":["# LSTM ëª¨ë¸ ì •ì˜\n","class DecoderRNN(nn.Module):\n","    def __init__(self, lstm_hidden_size, fc_hidden_size, dropout_rate):\n","        super(DecoderRNN, self).__init__()\n","\n","        self.RNN_input_size = len(cfg.features)\n","        self.h_RNN_layers = cfg.lstm_num_layers\n","        self.h_RNN = lstm_hidden_size\n","        self.h_FC_dim = fc_hidden_size\n","        self.drop_p = dropout_rate\n","        self.num_classes = len(cfg.labels)\n","        self.bidirectional = cfg.bidirectional\n","\n","        self.LSTM = nn.LSTM(\n","            input_size=self.RNN_input_size,\n","            hidden_size=self.h_RNN,\n","            num_layers=self.h_RNN_layers,\n","            batch_first=True,\n","            bidirectional=self.bidirectional\n","        )\n","\n","        lstm_output_dim = self.h_RNN * (2 if self.bidirectional else 1)\n","\n","        self.attention = Attention(lstm_output_dim) # Attention ì¶”ê°€\n","        self.fc1 = nn.Linear(lstm_output_dim, self.h_FC_dim)\n","        self.bn1 = nn.BatchNorm1d(self.h_FC_dim)  # Batch Normalization ì¶”ê°€\n","        self.fc2 = nn.Linear(self.h_FC_dim, self.num_classes)\n","        self.dropout = nn.Dropout(p=self.drop_p)\n","\n","    def forward(self, x_RNN):\n","        self.LSTM.flatten_parameters()\n","        out, _ = self.LSTM(x_RNN)  # (batch, seq_len, hidden*2)\n","\n","        attn_out, attn_weights = self.attention(out)  # attn_out: [B, H]\n","\n","        # FC layers\n","        x = self.fc1(attn_out)\n","        x = self.bn1(x)\n","        x = F.relu(x)\n","        x = self.dropout(x)\n","        x = self.fc2(x)\n","\n","        return x, attn_weights  # CrossEntropyLoss ì‚¬ìš© (Softmax ì—†ìŒ), attn_weights í•¨ê»˜ ë°˜í™˜"]},{"cell_type":"markdown","metadata":{"id":"RXocvVKtkOMG"},"source":["### optimizer"]},{"cell_type":"markdown","source":["- ëª¨ë¸ êµ¬ì¡° ë¹„êµ: Adam ê³ ì •\n","- ì •ê·œí™”/íŠœë‹ ë‹¨ê³„: Adam, AdamW, SGD ë¹„êµ\n","- ìµœì¢… ì„±ëŠ¥ ê·¹ëŒ€í™”: weight decay, scheduler, mixed precision ë“±ê³¼ ì¡°í•© ì‹¤í—˜"],"metadata":{"id":"JX_Y9TybhXI6"}},{"cell_type":"code","source":["def get_optimizer(model, optimizer_name, learning_rate):\n","    if optimizer_name == \"adam\":\n","        return optim.Adam(model.parameters(), lr=learning_rate)\n","    elif optimizer_name == \"sgd\":\n","        return optim.SGD(model.parameters(), lr=learning_rate, momentum=0.9)\n","    elif optimizer_name == \"adamw\":\n","        return optim.AdamW(model.parameters(), lr=learning_rate, weight_decay=0.01)\n","    else:\n","        raise ValueError(f\"Unsupported optimizer: {cfg.optimizer}\")"],"metadata":{"id":"3gHz7kiLhVVh","executionInfo":{"status":"ok","timestamp":1751349605135,"user_tz":-540,"elapsed":39,"user":{"displayName":"Sol Lee","userId":"04508513172834796855"}}},"execution_count":14,"outputs":[]},{"cell_type":"markdown","source":["### loss function (get_criterion)"],"metadata":{"id":"t6dLEyh4-SgG"}},{"cell_type":"code","source":["def get_criterion(class_weights=None):\n","    print(\"âœ… Using Weighted CrossEntropyLoss\")\n","    return nn.CrossEntropyLoss(weight=class_weights)"],"metadata":{"id":"gAQBS_KI-TlY","executionInfo":{"status":"ok","timestamp":1751349605136,"user_tz":-540,"elapsed":13,"user":{"displayName":"Sol Lee","userId":"04508513172834796855"}}},"execution_count":15,"outputs":[]},{"cell_type":"markdown","source":["### í•™ìŠµ ìŠ¤ì¼€ì¤„ëŸ¬ ì„¤ì •"],"metadata":{"id":"HjZoYJFvffDM"}},{"cell_type":"code","source":["def get_scheduler(optimizer):\n","    return optim.lr_scheduler.ReduceLROnPlateau(\n","        optimizer,\n","        mode=\"min\",  # 'min' for loss, 'max' for accuracy/f1\n","        factor=cfg.reduce_lr_factor,\n","        patience=cfg.reduce_lr_patience,\n",")"],"metadata":{"id":"svwn6KqlfiK_","executionInfo":{"status":"ok","timestamp":1751349605136,"user_tz":-540,"elapsed":4,"user":{"displayName":"Sol Lee","userId":"04508513172834796855"}}},"execution_count":16,"outputs":[]},{"cell_type":"markdown","metadata":{"id":"ne6SnCpE5sMd"},"source":["### Early Stopping ì„¤ì •"]},{"cell_type":"code","source":["class EarlyStopping:\n","    def __init__(self, patience, mode: str = \"min\"):\n","        self.patience = patience\n","        self.mode = mode\n","        self.best_score = np.inf if mode == \"min\" else -np.inf\n","        self.counter = 0\n","        self.early_stop = False\n","\n","    def step(self, current_score: float) -> bool:\n","        \"\"\"\n","        í‰ê°€ ì ìˆ˜(current_score)ë¥¼ ë°›ì•„ early stopping ì—¬ë¶€ë¥¼ ì—…ë°ì´íŠ¸í•¨.\n","        Returns:\n","            True if training should stop\n","        \"\"\"\n","        if self.mode == \"min\":\n","            is_improvement = current_score < self.best_score\n","        else:  # \"max\"\n","            is_improvement = current_score > self.best_score\n","\n","        if is_improvement:\n","            self.best_score = current_score\n","            self.counter = 0\n","        else:\n","            self.counter += 1\n","            if self.counter >= self.patience:\n","                self.early_stop = True\n","\n","        return self.early_stop"],"metadata":{"id":"cOHa2ZkNgW2U","executionInfo":{"status":"ok","timestamp":1751349605136,"user_tz":-540,"elapsed":3,"user":{"displayName":"Sol Lee","userId":"04508513172834796855"}}},"execution_count":17,"outputs":[]},{"cell_type":"markdown","metadata":{"id":"0XTNDoMjMh0Q"},"source":["## 1-4 í•™ìŠµ ì§„í–‰"]},{"cell_type":"markdown","metadata":{"id":"uX8o8vwQMlXU"},"source":["-  Gradient Clipping ì¶”ê°€ (torch.nn.utils.clip_grad_norm_()) â†’ í­ë°œì ì¸ ê¸°ìš¸ê¸°(Gradient Explosion) ë°©ì§€\n","- ëª¨ë¸ ì €ì¥ ê¸°ëŠ¥ ì¶”ê°€ (torch.save()) â†’ Best Loss ê°±ì‹  ì‹œ ìë™ ì €ì¥"]},{"cell_type":"code","execution_count":18,"metadata":{"id":"0R88mx_dea0I","executionInfo":{"status":"ok","timestamp":1751349605136,"user_tz":-540,"elapsed":2,"user":{"displayName":"Sol Lee","userId":"04508513172834796855"}}},"outputs":[],"source":["def save_model(model, optimizer, epoch, loss, fold, model_type): # model_type: \"best\", \"last\"\n","    model_filename = f\"{cfg.model_save_path}/{model_type}_model_{fold}.pth\"  # ì €ì¥ íŒŒì¼ëª…\n","\n","    torch.save({\n","        'epoch': epoch,\n","        'model_state_dict': model.state_dict(),\n","        'optimizer_state_dict': optimizer.state_dict(),\n","        'loss': loss\n","    }, model_filename)\n","\n","    print(f\"ğŸ’¾ Model Updated! Saved at {model_filename} (Loss: {loss:.4f})\")\n","\n","def save_training_results(\n","        train_losses,\n","        train_scores,\n","        val_losses,\n","        val_scores,\n","        fold\n","        ):\n","    \"\"\"\n","    í•™ìŠµ ê³¼ì •ì˜ ì†ì‹¤(loss)ê³¼ ì •í™•ë„(score)ë¥¼ ì €ì¥í•˜ëŠ” í•¨ìˆ˜\n","    \"\"\"\n","    results_df = pd.DataFrame({\n","        \"epoch\": list(range(1, len(train_losses) + 1)),\n","        \"train_loss\": train_losses,\n","        \"train_accuracy\": train_scores,\n","        \"val_loss\": val_losses,\n","        \"val_accuracy\": val_scores\n","    })\n","\n","    results_df.to_csv(f\"{cfg.results_save_path}/training_results_{fold}.csv\", index=False)\n","    print(f\"ğŸ’¾ Training results saved at {cfg.results_save_path}/training_results_{fold}.csv\")"]},{"cell_type":"markdown","metadata":{"id":"B3TL3Dy7bxnt"},"source":["### epochë§ˆë‹¤ validation dataë¡œ ì„±ëŠ¥í‰ê°€\n","- ë‹¤ìŒ ê¸°ìˆ ì„ ì ìš©í•˜ëŠ” ê¸°ì¤€: Validation data Loss\n","    - validation data ê¸°ì¤€ìœ¼ë¡œ best model ì„ ì •\n","    - ëª¨ë¸ ì €ì¥ (Best Loss ê°±ì‹  ì‹œ)"]},{"cell_type":"markdown","source":["### train & validate"],"metadata":{"id":"ve-1oy5X7SVB"}},{"cell_type":"code","source":["def train(model, device, train_loader, optimizer, criterion, epoch):\n","    model.train()\n","    losses, scores = [], []\n","\n","    for X, y in tqdm(train_loader, desc=f\"Epoch {epoch+1} [Train]\"):\n","        X, y = X.to(device), y.to(device).long()\n","\n","        optimizer.zero_grad()\n","        output = model(X)\n","\n","        # íŠœí”Œ ì²˜ë¦¬\n","        if isinstance(output, tuple):\n","            output = output[0]\n","\n","        loss = criterion(output, y)\n","        loss.backward() # ì—­ì „íŒŒ\n","\n","        torch.nn.utils.clip_grad_norm_(model.parameters(), max_norm=1.0)  #  Gradient Clipping\n","        optimizer.step()\n","\n","        losses.append(loss.item())\n","\n","        # ì •í™•ë„ ê³„ì‚°\n","        y_pred = torch.argmax(output, dim=1)\n","        acc = accuracy_score(y.cpu().numpy(), y_pred.cpu().numpy())\n","        scores.append(acc)\n","\n","    avg_loss = sum(losses) / len(losses)\n","    avg_acc = sum(scores) / len(scores) * 100\n","\n","    print(f\"âœ… Train Epoch {epoch+1} Finished - Avg Loss: {avg_loss:.4f}, Avg Acc: {avg_acc:.2f}%\")\n","\n","    return avg_loss, avg_acc  # í•™ìŠµ Loss ë° Accuracy ë°˜í™˜\n","\n","def validate(model, device, criterion, val_loader):\n","    model.eval()\n","    losses, scores = [], []\n","\n","    with torch.no_grad():\n","        for X, y in val_loader:\n","            X, y = X.to(device), y.to(device).long()\n","            output = model(X)\n","\n","            # íŠœí”Œ ì²˜ë¦¬\n","            if isinstance(output, tuple):\n","                output = output[0]\n","            loss = criterion(output, y)\n","\n","            losses.append(loss.item())\n","\n","            y_pred = torch.argmax(output, dim=1)\n","            acc = accuracy_score(y.cpu().numpy(), y_pred.cpu().numpy())\n","            scores.append(acc)\n","\n","    avg_loss = sum(losses) / len(losses)\n","    avg_acc = sum(scores) / len(scores) * 100\n","\n","    print(f\"âœ… Validation Finished - Avg Loss: {avg_loss:.4f}, Avg Acc: {avg_acc:.2f}%\")\n","\n","    return avg_loss, avg_acc  # ê²€ì¦ Loss ë° Accuracy ë°˜í™˜"],"metadata":{"id":"cb6g45rd7Td3","executionInfo":{"status":"ok","timestamp":1751349605161,"user_tz":-540,"elapsed":26,"user":{"displayName":"Sol Lee","userId":"04508513172834796855"}}},"execution_count":19,"outputs":[]},{"cell_type":"code","source":["# ì „ì²´ í•™ìŠµ ì‹¤í–‰ ì½”ë“œ\n","def train_and_validate(model, train_loader, val_loader, optimizer, criterion, fold, earlystop_patience=cfg.early_stopping_patience, epochs=cfg.epochs, scheduler=None):\n","    best_loss = np.inf\n","\n","    train_losses, train_scores = [], []\n","    val_losses, val_scores = [], []\n","\n","    # EarlyStopping ê°ì²´ ìƒì„±\n","    early_stopper = EarlyStopping(earlystop_patience, mode=\"min\")\n","\n","    for epoch in range(epochs):\n","        # í•™ìŠµ ë‹¨ê³„\n","        train_loss, train_score = train(\n","            model=model,\n","            device=cfg.device,\n","            train_loader=train_loader,\n","            optimizer=optimizer,\n","            criterion=criterion,\n","            epoch=epoch\n","        )\n","\n","        val_loss, val_score = validate(\n","            model=model,\n","            device=cfg.device,\n","            criterion=criterion,\n","            val_loader=val_loader\n","        )\n","\n","        train_losses.append(train_loss)\n","        train_scores.append(train_score)\n","        val_losses.append(val_loss)\n","        val_scores.append(val_score)\n","\n","        # í•™ìŠµë¥  ê°ì†Œ ì ìš©\n","        if cfg.use_lr_scheduler:\n","          pass\n","\n","        # Best model ì €ì¥\n","        if val_loss < best_loss:\n","            best_loss = val_loss\n","            patience_counter = 0\n","\n","            save_model(\n","                model=model,\n","                optimizer=optimizer,\n","                epoch=epoch,\n","                loss=best_loss,\n","                fold=fold,\n","                model_type=\"best\"\n","            )\n","\n","            print(f\"Best Loss Updated! : {best_loss}\")\n","\n","        # Early Stopping ì¶”ê°€\n","        if cfg.use_early_stopping:\n","            if early_stopper.step(val_loss):\n","                print(f\"Early stopping triggered at epoch {epoch+1}\")\n","                break\n","\n","    # ë§ˆì§€ë§‰ ëª¨ë¸ ì €ì¥\n","    save_model(\n","      model=model,\n","      optimizer=optimizer,\n","      epoch=epoch,\n","      loss=val_loss,\n","      fold=fold,\n","      model_type=\"last\"\n","    )\n","\n","    # í•™ìŠµ ê²°ê³¼ ì €ì¥\n","    save_training_results(\n","      train_losses,\n","      train_scores,\n","      val_losses,\n","      val_scores,\n","      fold=fold\n","    )\n","\n","    return train_losses, train_scores, val_losses, val_scores"],"metadata":{"id":"IWnfoFlV9LHG","executionInfo":{"status":"ok","timestamp":1751349605169,"user_tz":-540,"elapsed":1,"user":{"displayName":"Sol Lee","userId":"04508513172834796855"}}},"execution_count":20,"outputs":[]},{"cell_type":"markdown","source":["### optuna"],"metadata":{"id":"_qDJft2BtC95"}},{"cell_type":"code","source":["!pip install optuna"],"metadata":{"id":"C-fWLU3AupJC","executionInfo":{"status":"ok","timestamp":1751281121313,"user_tz":-540,"elapsed":7271,"user":{"displayName":"Sol Lee","userId":"06456628920237989185"}},"colab":{"base_uri":"https://localhost:8080/"},"outputId":"16d67481-d9bc-4143-fdbb-75b95d46d6ff"},"execution_count":null,"outputs":[{"output_type":"stream","name":"stdout","text":["Collecting optuna\n","  Downloading optuna-4.4.0-py3-none-any.whl.metadata (17 kB)\n","Collecting alembic>=1.5.0 (from optuna)\n","  Downloading alembic-1.16.2-py3-none-any.whl.metadata (7.3 kB)\n","Collecting colorlog (from optuna)\n","  Downloading colorlog-6.9.0-py3-none-any.whl.metadata (10 kB)\n","Requirement already satisfied: numpy in /usr/local/lib/python3.11/dist-packages (from optuna) (2.0.2)\n","Requirement already satisfied: packaging>=20.0 in /usr/local/lib/python3.11/dist-packages (from optuna) (24.2)\n","Requirement already satisfied: sqlalchemy>=1.4.2 in /usr/local/lib/python3.11/dist-packages (from optuna) (2.0.41)\n","Requirement already satisfied: tqdm in /usr/local/lib/python3.11/dist-packages (from optuna) (4.67.1)\n","Requirement already satisfied: PyYAML in /usr/local/lib/python3.11/dist-packages (from optuna) (6.0.2)\n","Requirement already satisfied: Mako in /usr/lib/python