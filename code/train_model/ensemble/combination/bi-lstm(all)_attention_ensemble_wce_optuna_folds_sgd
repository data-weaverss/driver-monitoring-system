   axes[fold][0].set_xlabel('Epoch')\n","        axes[fold][0].set_ylabel('Loss')\n","        axes[fold][0].legend()\n","        axes[fold][0].grid(True)\n","\n","        # Score\n","        axes[fold][1].plot(epochs, all_train_scores[fold], label='Train Score', marker='o')\n","        axes[fold][1].plot(epochs, all_val_scores[fold], label='Val Score', marker='s', linestyle='--')\n","        axes[fold][1].set_title(f'Fold {fold} - Score')\n","        axes[fold][1].set_xlabel('Epoch')\n","        axes[fold][1].set_ylabel('Score')\n","        axes[fold][1].legend()\n","        axes[fold][1].grid(True)\n","\n","    plt.tight_layout()\n","    plot_path = os.path.join(cfg.results_save_path, f\"trainval_curve.png\")\n","    plt.savefig(plot_path, dpi=300)\n","    plt.show()\n","\n","    print(f\"ğŸ“Š Foldë³„ í•™ìŠµ ê²°ê³¼ ì‹œê°í™” ì €ì¥ ì™„ë£Œ: {plot_path}\")\n"],"metadata":{"id":"llgQuP59NYOQ","executionInfo":{"status":"ok","timestamp":1751349730903,"user_tz":-540,"elapsed":32,"user":{"displayName":"Sol Lee","userId":"04508513172834796855"}}},"execution_count":21,"outputs":[]},{"cell_type":"code","source":["plot_train_val_results_per_fold(\n","    all_train_losses,\n","    all_train_scores,\n","    all_val_losses,\n","    all_val_scores\n",")"],"metadata":{"id":"072p6ozVwuth","colab":{"base_uri":"https://localhost:8080/","height":216},"executionInfo":{"status":"error","timestamp":1751349730941,"user_tz":-540,"elapsed":38,"user":{"displayName":"Sol Lee","userId":"04508513172834796855"}},"outputId":"570c40f6-16f9-4c4d-e1ed-a512e8968783"},"execution_count":22,"outputs":[{"output_type":"error","ename":"NameError","evalue":"name 'all_train_losses' is not defined","traceback":["\u001b[0;31m---------------------------------------------------------------------------\u001b[0m","\u001b[0;31mNameError\u001b[0m                                 Traceback (most recent call last)","\u001b[0;32m/tmp/ipython-input-22-369624845.py\u001b[0m in \u001b[0;36m<cell line: 0>\u001b[0;34m()\u001b[0m\n\u001b[1;32m      1\u001b[0m plot_train_val_results_per_fold(\n\u001b[0;32m----> 2\u001b[0;31m     \u001b[0mall_train_losses\u001b[0m\u001b[0;34m,\u001b[0m\u001b[0;34m\u001b[0m\u001b[0;34m\u001b[0m\u001b[0m\n\u001b[0m\u001b[1;32m      3\u001b[0m     \u001b[0mall_train_scores\u001b[0m\u001b[0;34m,\u001b[0m\u001b[0;34m\u001b[0m\u001b[0;34m\u001b[0m\u001b[0m\n\u001b[1;32m      4\u001b[0m     \u001b[0mall_val_losses\u001b[0m\u001b[0;34m,\u001b[0m\u001b[0;34m\u001b[0m\u001b[0;34m\u001b[0m\u001b[0m\n\u001b[1;32m      5\u001b[0m     \u001b[0mall_val_scores\u001b[0m\u001b[0;34m\u001b[0m\u001b[0;34m\u001b[0m\u001b[0m\n","\u001b[0;31mNameError\u001b[0m: name 'all_train_losses' is not defined"]}]},{"cell_type":"markdown","metadata":{"id":"RvogPajT106l"},"source":["## confusion matrix, classification_report ì‹œê°í™”"]},{"cell_type":"code","execution_count":23,"metadata":{"id":"PgKzQHFH84m0","executionInfo":{"status":"ok","timestamp":1751349739647,"user_tz":-540,"elapsed":51,"user":{"displayName":"Sol Lee","userId":"04508513172834796855"}}},"outputs":[],"source":["def load_all_best_models(lstm_hidden_size, fc_hidden_size, dropout_rate, device) -> List[torch.nn.Module]:\n","    \"\"\"í´ë” ë‚´ ëª¨ë“  best_model_*.pth íŒŒì¼ì„ ë¶ˆëŸ¬ì™€ ëª¨ë¸ ë¦¬ìŠ¤íŠ¸ ë°˜í™˜\"\"\"\n","    model_list = []\n","    pattern = os.path.join(cfg.model_save_path, \"best_model_*.pth\")\n","    model_paths = sorted(glob.glob(pattern))\n","\n","    for path in model_paths:\n","        model = DecoderRNN(lstm_hidden_size, fc_hidden_size, dropout_rate).to(device)\n","        checkpoint = torch.load(path, map_location=device)\n","        model.load_state_dict(checkpoint[\"model_state_dict\"])\n","        model.eval()\n","        model_list.append(model)\n","        print(f\"âœ… Loaded: {os.path.basename(path)}\")\n","\n","    return model_list\n","\n","# Confusion Matrix ì‹œê°í™” í•¨ìˆ˜\n","def plot_confusion_matrix_and_report(\n","    y_true,\n","    y_pred,\n","    data_type,\n","    model_type=ModelType.BEST\n","    ):\n","    \"\"\"\n","    Confusion Matrixë¥¼ ê·¸ë¦¬ëŠ” í•¨ìˆ˜\n","    \"\"\"\n","    # == confusion matrix ==\n","    cm = confusion_matrix(y_true, y_pred)\n","    plt.figure(figsize=(6, 6))\n","    sns.heatmap(cm, annot=True, fmt='d', cmap='Blues', xticklabels=cfg.labels, yticklabels=cfg.labels)\n","    plt.xlabel(\"Predicted Label\")\n","    plt.ylabel(\"True Label\")\n","    plt.title(\"Confusion Matrix\")\n","\n","    file_prefix = f\"{model_type.value}_{data_type.value}\"\n","    cm_path = os.path.join(cfg.results_save_path, f\"{file_prefix}_confusion_matrix.png\")\n","    plt.savefig(cm_path, dpi=300)  # ì´ë¯¸ì§€ ì €ì¥\n","    plt.show()\n","    plt.close()\n","    print(f\"âœ… Confusion Matrix saved at: {cm_path}\")\n","\n","    # == classification report ==\n","    report = classification_report(y_true, y_pred, target_names=cfg.labels, output_dict=True)\n","    report_df = pd.DataFrame(report).transpose()\n","\n","    # ì‹œê°í™”\n","    fig, ax = plt.subplots(figsize=(10, len(report_df) * 0.6))\n","    ax.axis('off')\n","\n","    table = ax.table(\n","        cellText=report_df.round(3).values,\n","        colLabels=report_df.columns,\n","        rowLabels=report_df.index,\n","        loc='center',\n","        cellLoc='center'\n","    )\n","\n","    # í—¤ë” ì„¤ì •\n","    for j in range(len(report_df.columns)):\n","        cell = table[0, j]\n","        cell.set_facecolor('#d0d0d0')\n","        cell.set_fontsize(13)\n","        cell.set_text_props(weight='bold')\n","\n","    # ì…€ ìŠ¤íƒ€ì¼ ì¡°ì •\n","    for i in range(len(report_df)):\n","        for j in range(len(report_df.columns)):\n","            cell = table[i + 1, j]\n","            cell.set_fontsize(12)\n","            cell.set_height(0.09)\n","            cell.set_facecolor('#a9a9a9' if i % 2 == 0 else '#ffffff')\n","\n","    # í–‰ ë¼ë²¨ ìŠ¤íƒ€ì¼ ì¡°ì •\n","    for i in range(len(report_df)):\n","        row_label = table[i + 1, -1] if (i + 1, -1) in table._cells else table[i + 1, 0]\n","        row_label.set_fontsize(12)\n","        row_label.set_text_props(weight='bold')\n","        row_label.set_height(0.09)\n","\n","    cr_path = os.path.join(cfg.results_save_path, f\"{file_prefix}_classification_report.png\")\n","    plt.savefig(cr_path, dpi=300, bbox_inches='tight')\n","    plt.show()\n","    plt.close()\n","    print(f\"ğŸ“‹ Classification Report table saved at: {cr_path}\")\n","\n","\n","def evaluate_models(models: List[torch.nn.Module], device, data_loader, data_type, model_type=ModelType.BEST):\n","    \"\"\"\n","    ì—¬ëŸ¬ ëª¨ë¸ì˜ softmax í™•ë¥ ì„ í‰ê·  ë‚´ì–´ ì˜ˆì¸¡í•˜ëŠ” ì•™ìƒë¸” í‰ê°€ í•¨ìˆ˜\n","    \"\"\"\n","    for model in models:\n","        model.eval()\n","\n","    all_preds = []\n","    all_labels = []\n","    all_probs = []\n","\n","    with torch.no_grad():\n","        for x_batch, y_batch in data_loader:\n","            x_batch, y_batch = x_batch.to(device), y_batch.to(device)\n","\n","            # ëª¨ë¸ë³„ í™•ë¥  ì˜ˆì¸¡ê°’ ì €ì¥\n","            ensemble_probs = []\n","\n","            for model in models:\n","                logits = model(x_batch)\n","                if isinstance(logits, tuple):\n","                    logits = logits[0]\n","                probs = F.softmax(logits, dim=1)\n","                ensemble_probs.append(probs)\n","\n","            # í™•ë¥  í‰ê·  (batch_size, num_classes)\n","            avg_probs = torch.stack(ensemble_probs).mean(dim=0)\n","\n","            y_pred_classes = torch.argmax(avg_probs, dim=1)\n","\n","            all_preds.extend(y_pred_classes.cpu().numpy())\n","            all_labels.extend(y_batch.cpu().numpy())\n","            all_probs.extend(avg_probs.cpu().numpy())\n","\n","    # Confusion Matrix ë° Classification Report\n","    plot_confusion_matrix_and_report(all_labels, all_preds, data_type, model_type)\n","\n","    return all_labels, all_preds, np.array(all_probs)"]},{"cell_type":"code","source":["models = load_all_best_models(cfg.lstm_hidden_size, cfg.fc_hidden_size, cfg.dropout_rate, cfg.device) # best model load"],"metadata":{"id":"x70cCjt985w2","executionInfo":{"status":"ok","timestamp":1751349792567,"user_tz":-540,"elapsed":35317,"user":{"displayName":"Sol Lee","userId":"04508513172834796855"}},"colab":{"base_uri":"https://localhost:8080/"},"outputId":"fac413f4-28ff-4b16-e9b8-c12aaba421a0"},"execution_count":28,"outputs":[{"output_type":"stream","name":"stdout","text":["âœ… Loaded: best_model_0.pth\n","âœ… Loaded: best_model_1.pth\n","âœ… Loaded: best_model_2.pth\n","âœ… Loaded: best_model_3.pth\n","âœ… Loaded: best_model_4.pth\n"]}]},{"cell_type":"code","source":["eval_params = train_params.copy()\n","\n","# í‰ê°€ìš©ì€ drop_last = Falseë¡œ ì„¤ì •\n","eval_params['drop_last'] = False"],"metadata":{"id":"JFWq7dK0Hrf1","executionInfo":{"status":"error","timestamp":1751349792590,"user_tz":-540,"elapsed":22,"user":{"displayName":"Sol Lee","userId":"04508513172834796855"}},"colab":{"base_uri":"https://localhost:8080/","height":198},"outputId":"3cfd51c0-2404-4517-fbc6-e51648923b34"},"execution_count":29,"outputs":[{"output_type":"error","ename":"NameError","evalue":"name 'train_params' is not defined","traceback":["\u001b[0;31m---------------------------------------------------------------------------\u001b[0m","\u001b[0;31mNameError\u001b[0m                                 Traceback (most recent call last)","\u001b[0;32m/tmp/ipython-input-29-3072004831.py\u001b[0m in \u001b[0;36m<cell line: 0>\u001b[0;34m()\u001b[0m\n\u001b[0;32m----> 1\u001b[0;31m \u001b[0meval_params\u001b[0m \u001b[0;34m=\u001b[0m \u001b[0mtrain_params\u001b[0m\u001b[0;34m.\u001b[0m\u001b[0mcopy\u001b[0m\u001b[0;34m(\u001b[0m\u001b[0;34m)\u001b[0m\u001b[0;34m\u001b[0m\u001b[0;34m\u001b[0m\u001b[0m\n\u001b[0m\u001b[1;32m      2\u001b[0m \u001b[0;34m\u001b[0m\u001b[0m\n\u001b[1;32m      3\u001b[0m \u001b[0;31m# í‰ê°€ìš©ì€ drop_last = Falseë¡œ ì„¤ì •\u001b[0m\u001b[0;34m\u001b[0m\u001b[0;34m\u001b[0m\u001b[0m\n\u001b[1;32m      4\u001b[0m \u001b[0meval_params\u